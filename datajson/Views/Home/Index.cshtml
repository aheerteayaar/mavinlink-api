        @model datajson.Models.Welcome
        @{
            ViewData["Title"] = "Home Page";
        }

        @{ if (Model is not null)
            {
                foreach (var item in Model.Workspaces)
                {

            <details>
                <summary><strong>@item.Key :</strong> @item.Value.Title</summary>
        
                <h4> <strong>Project Name -> </strong> @item.Value.Title </h4>



                @{
                            <h5>Associated Cleints:</h5>
                    for (int i = 0; i < item.Value.ParticipantIds.Count(); i++)
                    {

                        var _userrs = Model.Users.Where(x => x.Value.Id == item.Value.ParticipantIds[i]).FirstOrDefault();
                        <ul>
                                    
                            <li>

                                 @_userrs.Value.FullName
                            </li>
                        </ul>
                    }
                }

                </details>
                        }
                        }
                
                        }
